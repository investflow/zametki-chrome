{
  "description": "Support for personal notes (zametki) on Investflow.Ru service",
  "background": {
    "page": "background.htm",
    "persistent": true
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "icons": {
    "16": "logo_16.png"
  },
  "manifest_version": 2,
  "name": "Заметки для Investflow",
  "permissions": [
    "contextMenus",
    "tabs"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.0.0"
}
